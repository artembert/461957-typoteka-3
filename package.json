{
  "name": "typoteka-3",
  "version": "1.0.0",
  "description": "Типотека",
  "main": "./src/service/service.ts",
  "scripts": {
    "start": "ts-node -r dotenv/config src/service/service.ts",
    "start:prod": "cross-env NODE_ENV=production node -r dotenv/config build/service/service.js",
    "start-front-server": "ts-node -r dotenv/config src/express/index.ts",
    "start-front-server:prod": "cross-env NODE_ENV=production node -r dotenv/config build/express/index.js",
    "build": "npm run build:ts && npm run copy-static-assets",
    "build:ts": "tsc",
    "copy-static-assets": "node copy-static-assets.js",
    "test": "npm run eslint",
    "test:rest": "cross-env PORT=7777 jest --maxWorkers=1 src/service/cli/server",
    "eslint": "eslint --ext .js,.ts src/",
    "nodemon": "npm run nodemon:data & npm run nodemon:front",
    "nodemon:data": "nodemon --config nodemon.data.json",
    "nodemon:front": "nodemon --config nodemon.front.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artembert/461957-typoteka-3.git"
  },
  "author": "Artem Beresnev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/artembert/461957-typoteka-3/issues"
  },
  "homepage": "https://github.com/artembert/461957-typoteka-3#readme",
  "devDependencies": {
    "@types/express": "4.17.3",
    "@types/jest": "26.0.0",
    "@types/multer": "1.4.3",
    "@types/node": "13.5.0",
    "@types/prop-types": "15.7.3",
    "@types/react": "16.9.41",
    "@types/react-dom": "16.9.8",
    "@types/supertest": "2.0.9",
    "@typescript-eslint/eslint-plugin": "2.17.0",
    "@typescript-eslint/parser": "2.17.0",
    "babel-eslint": "10.1.0",
    "eslint": "^7.3.1",
    "eslint-config-htmlacademy": "0.5.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-react": "^7.20.3",
    "jest": "26.0.1",
    "nanoid": "3.1.10",
    "nodemon": "2.0.2",
    "package-json-type": "1.0.3",
    "prettier": "1.19.1",
    "shelljs": "0.8.4",
    "supertest": "4.0.2",
    "ts-jest": "26.1.0",
    "ts-node": "8.6.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "axios": "0.19.2",
    "cross-env": "7.0.2",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "multer": "1.4.2",
    "prop-types": "15.7.2",
    "pug": "2.0.4",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  }
}
