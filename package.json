{
  "name": "typoteka-3",
  "version": "1.0.0",
  "description": "Типотека",
  "main": "src/service/index.ts",
  "scripts": {
    "start": "npm run ts-node -- --project src/service/tsconfig.json src/service/index.ts --server | pino-pretty",
    "start:debug": "cross-env LOG_LEVEL=debug npm run start",
    "start:prod": "cross-env NODE_ENV=production node -r dotenv/config build/service/service.js",
    "start-front-server": "npm run ts-node -- --project src/express/tsconfig.json src/express/index.ts",
    "start-front-server:prod": "cross-env NODE_ENV=production node -r dotenv/config build/express/index.js",
    "build": "npm run build:ts && npm run copy-static-assets",
    "build:ts": "npm run build:service && npm run build:front",
    "build:front": "tsc --build src/express --verbose",
    "build:service": "tsc --build src/service --verbose",
    "copy-static-assets": "node copy-static-assets.js",
    "lint": "npm run eslint",
    "test:rest": "npm run generate-mock && npm run fill-db && cross-env PORT=7777 LOG_LEVEL=error jest --maxWorkers=1 src/service/server",
    "generate-mock": "ts-node src/service/index.ts --generate",
    "generate-database-mock": "ts-node --transpile-only src/service/index.ts --fill",
    "fill-db": "npm run ts-node -- src/service/index.ts --fill-db 50 | pino-pretty",
    "eslint": "eslint --ext .js,.ts src/",
    "nodemon": "./node_modules/.bin/nodemon",
    "nodemon:data": "npm run nodemon -- --config nodemon.data.json",
    "nodemon:front": "npm run nodemon -- --config nodemon.front.json",
    "npm-check-updates": "./node_modules/.bin/ncu",
    "connect-to-database": "npm run ts-node -- src/service/server/data-access/database-connector.ts | pino-pretty",
    "/": "___UTILITY_COMMANDS___",
    "ts-node": "./node_modules/.bin/ts-node --transpile-only -r dotenv/config"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artembert/461957-typoteka-3.git"
  },
  "author": "Artem Beresnev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/artembert/461957-typoteka-3/issues"
  },
  "homepage": "https://github.com/artembert/461957-typoteka-3#readme",
  "devDependencies": {
    "@types/express": "4.17.9",
    "@types/jest": "26.0.19",
    "@types/multer": "1.4.5",
    "@types/node": "14.14.14",
    "@types/pg": "7.14.7",
    "@types/pino": "6.3.4",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.10.0",
    "@typescript-eslint/parser": "4.10.0",
    "babel-eslint": "10.1.0",
    "eslint": "7.15.0",
    "eslint-config-htmlacademy": "0.6.0",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-react": "7.21.5",
    "jest": "26.6.3",
    "nodemon": "2.0.6",
    "npm-check-updates": "10.2.5",
    "package-json-type": "1.0.3",
    "pino-pretty": "4.3.0",
    "prettier": "2.2.1",
    "shelljs": "0.8.4",
    "sqlite3": "5.0.1",
    "supertest": "6.0.1",
    "ts-jest": "26.4.4",
    "ts-node": "9.1.1",
    "typescript": "4.1.3"
  },
  "dependencies": {
    "axios": "0.21.0",
    "cross-env": "7.0.3",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "multer": "1.4.2",
    "nanoid": "3.1.20",
    "pg": "8.5.1",
    "pino": "6.8.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "sequelize": "6.3.5"
  }
}
