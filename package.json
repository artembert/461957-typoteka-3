{
  "name": "typoteka-3",
  "version": "1.0.0",
  "description": "Типотека",
  "main": "./src/service/service.ts",
  "scripts": {
    "start": "ts-node -r dotenv/config src/service/service.ts --server | pino-pretty",
    "start:debug": "cross-env LOG_LEVEL=debug npm run start",
    "start:prod": "cross-env NODE_ENV=production node -r dotenv/config build/service/service.js",
    "start-front-server": "ts-node -r dotenv/config src/express/index.ts",
    "start-front-server:prod": "cross-env NODE_ENV=production node -r dotenv/config build/express/index.js",
    "build": "npm run build:ts && npm run copy-static-assets",
    "build:ts": "tsc",
    "copy-static-assets": "node copy-static-assets.js",
    "test": "npm run eslint",
    "test:rest": "cross-env PORT=7777 jest --maxWorkers=1 src/service/cli/server",
    "generate-mock": "ts-node src/service/service.ts --generate",
    "eslint": "eslint --ext .js,.ts src/",
    "nodemon": "./node_modules/.bin/nodemon",
    "nodemon:data": "npm run nodemon -- --config nodemon.data.json",
    "nodemon:front": "npm run nodemon -- --config nodemon.front.json",
    "npm-check-updates": "./node_modules/.bin/ncu"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artembert/461957-typoteka-3.git"
  },
  "author": "Artem Beresnev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/artembert/461957-typoteka-3/issues"
  },
  "homepage": "https://github.com/artembert/461957-typoteka-3#readme",
  "devDependencies": {
    "@types/express": "4.17.7",
    "@types/jest": "26.0.10",
    "@types/multer": "1.4.4",
    "@types/node": "14.0.27",
    "@types/pino": "6.3.0",
    "@types/prop-types": "15.7.3",
    "@types/react": "16.9.46",
    "@types/react-dom": "16.9.8",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "babel-eslint": "10.1.0",
    "eslint": "^7.7.0",
    "eslint-config-htmlacademy": "0.5.2",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-react": "^7.20.6",
    "jest": "26.4.0",
    "nanoid": "3.1.12",
    "nodemon": "2.0.4",
    "npm-check-updates": "7.1.1",
    "package-json-type": "1.0.3",
    "pino-pretty": "4.1.0",
    "prettier": "2.0.5",
    "shelljs": "0.8.4",
    "supertest": "4.0.2",
    "ts-jest": "26.2.0",
    "ts-node": "8.10.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@sindresorhus/slugify": "^1.1.0",
    "axios": "0.19.2",
    "cross-env": "7.0.2",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "multer": "1.4.2",
    "pino": "6.5.1",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  }
}
